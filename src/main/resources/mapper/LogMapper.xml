<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.qydata.mapper.LogMapper">



    <insert id="createLog" parameterType="org.qydata.entity.log.Log" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO qyfinance.Log(type,title,remoteAddr,requestUri,method,params,beginTime,endTime,userId,createTime)
        VALUES(#{type},#{title},#{remoteAddr},#{requestUri},#{method},#{params},#{beginTime},#{endTime},#{userId},sysdate())
    </insert>

</mapper>

